<nav class="nav-bar">
		<ul class="nav-bar">
				<li id="title"><%= link_to "Photogur", "/pictures/" %></li>
				<li><%= link_to "New Picture", "/pictures/new" %></li>

</nav>


<div id="image-container">


	<% @pictures.each do |picture| %>
		<div class="box">

		<div class="pic-info">
			<%= picture[:title] %> , <%= picture[:artist] %>
		</div>

		<div class="image">
			<%= link_to image_tag(picture.url), "/pictures/#{picture.id}"%>

		</div>


		</div>

	<% end %>


</div>